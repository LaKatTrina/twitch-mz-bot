<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>TdF - <%= channel %> - Listado timeouts</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
<body>
<h3>Immunes 24h - <a href="https://twitch.tv/<%= channel %>" target="_blank"><%= channel %></a></h3>
<span></span>
<%- include('partials/menu', { current: 'immunes' }) %>
<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Usuario</th>
        <th scope="col">Expira en...</th>
        <th scope="col">Strikes</th>
    </tr>
    </thead>
    <tbody>
    <%  immunes.forEach(function(immune, index) { %>
        <tr class="table-light">
            <th scope="row">Slot <%= immune.slot %></th>
            <td><%= immune.username %></td>
            <td><%= immune.days + ' dias ' + immune.hours + ' horas ' + immune.minutes + ' minutos ' + immune.seconds + ' segundos'  %></td>
            <td><%= immune.bans %> de 3</td>
        </tr>
    <% }); %>
    </tbody>
</table>
<%- include('layout/footer') %>
